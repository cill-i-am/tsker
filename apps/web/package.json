{
  "name": "web",
  "private": true,
  "type": "module",
  "imports": {
    "#/*": "./src/*"
  },
  "scripts": {
    "dev": "portless app.tsker vite dev",
    "dev:e2e": "vite dev --host 0.0.0.0 --port 3000",
    "e2e:db:migrate": "pnpm --filter @repo/db drizzle:migrate",
    "e2e:start:api": "pnpm run e2e:db:migrate && pnpm --filter api dev:e2e",
    "e2e:start:auth": "pnpm --filter auth dev:e2e",
    "e2e:start:web": "pnpm run dev:e2e",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests src",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "ultracite check src/components src/lib src/routes src/router.tsx src/styles.css",
    "type-check": "tsc --noEmit",
    "check-types": "pnpm run type-check"
  },
  "dependencies": {
    "@base-ui/react": "^1.2.0",
    "@fontsource-variable/inter": "^5.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "better-auth": "^1.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.545.0",
    "next-themes": "^0.4.6",
    "nitro": "npm:nitro-nightly@latest",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-day-picker": "^9.13.2",
    "react-dom": "^19.2.0",
    "react-resizable-panels": "^4.6.5",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.1.18",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@tanstack/devtools-vite": "^0.3.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "jsdom": "^27.0.0",
    "shadcn": "^3.8.5",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  }
}
